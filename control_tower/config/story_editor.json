{
    "window": {
        "width": 1200,
        "height": 1800
    },
    "text_editor": {
        "font_family": "Segoe UI",
        "font_size": 11,
        "min_height": 80,
        "max_height": 300
    },
    "thumbnail": {
        "width": 128,
        "grid_columns": 2
    },
    "story_board": {
        "column": 5,
        "image_width": 256
    },
    "tspan": {
        "font_size": 24,
        "border_color": "#595959",
        "background_color": "#333333",
        "text_color": "#9C9C9C",
        "focus_border_color": "#8c8c8c",
        "selection_color": "#4A9EFF",
        "selection_text_color": "#C5C5C5"
    },
    "tooltip": {
        "background_color": "#cfd171",
        "text_color": "#120E0E",
        "border_color": "#101010"
    },
    "style_label": {
        "color": "#888",
        "font_size": 10
    },
    "text_element_header": {
        "color": "#4A9EFF",
        "font_size": 12
    },
    "layer_header": {
        "color": "#FFD700",
        "font_size": 14
    },
    "group_box": {
        "border_color": "#555",
        "background_color": "#1e1e1e",
        "hover_border_color": "#666"
    },
    "separator": {
        "color": "#555",
        "font_size": 10
    },
    "main_window": {
        "background_color": "#81623f"
    },
    "toolbar": {
        "background_color": "#8e764e"
    },
    "activate_button": {
        "disabled_color": "#A0A0A0",
        "disabled_bg": "#242424",
        "bg": "#aa805a",
        "color": "#000000",
        "checked_bg": "#a5a5a5",
        "checked_color": "#000000"
    },
    "template_combo": {
        "color": "black",
        "background_color": "#8e764e"
    },
    "find_replace_button": {
        "color": "#221c13",
        "font_size": "18px"
    },
    "scroll_area": {
        "border_color": "#555",
        "background_color": "#2b2b2b",
        "scrollbar_background": "#2b2b2b",
        "scrollbar_handle": "#555",
        "scrollbar_handle_hover": "#666"
    }
}